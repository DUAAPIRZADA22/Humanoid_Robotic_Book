{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://github.com"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/images/",
      "replacement": "https://raw.githubusercontent.com/your-org/humanoid-robotic-book/main/static/images/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://docs.nvidia.com"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36",
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 403]
}